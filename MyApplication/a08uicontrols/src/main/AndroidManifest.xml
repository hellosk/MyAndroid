<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hisunke.a08uicontrols">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UsingRadioGroup" />
        <activity android:name=".UsingCheckBox" />
        <activity android:name=".UsingDatePicker" />
        <activity android:name=".UsingTimePicker" />
        <activity android:name=".UsingSpinner" />
        <activity android:name=".UsingProgressBar" />
        <activity android:name=".UsingAutoCompleteTextView"></activity>
    </application>

</manifest>